    <section class="home-section about_sec" id="about">
      <div class="container">
        <div class="about_link_"><span>About The Exchange</span></div>
        <div class="inner_">
          <div class="img_wrap">
              <picture>
                <!-- <source srcset="assets/images/initiative.webp" type="image/webp">-->
